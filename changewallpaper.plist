<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
	<dict>
		<key>Label</key>
		<string>com.changewallpaper.app</string>
		<key>Program</key>
		<string>{update-path}/changewallpaper.sh</string>
		<key>RunAtLoad</key>
		<true/>
		<key>StartInterval</key>
		<integer>3600</integer>
		<key>StandardErrorPath</key>
		<string>{update-path}/error.log</string>
		<key>StandardOutPath</key>
		<string>{update-path}/stdout.log</string>
	</dict>
</plist>